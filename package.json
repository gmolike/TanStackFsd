{
  "name": "react-fsd-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Modern React 19 app with Feature-Sliced Design architecture",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "keywords": ["react", "typescript", "fsd", "vite", "tanstack"],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "_____________ DEVELOPMENT _____________": "",
    "dev": "vite --host",
    "dev:https": "vite --host --https",
    "dev:debug": "vite --debug --host",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "_____________ BUILD _____________": "",
    "prebuild": "pnpm run validate",
    "build": "tsc && vite build",
    "build:analyze": "VITE_BUILD_ANALYZE=true pnpm build",
    "build:staging": "NODE_ENV=staging pnpm build",
    "postbuild": "echo 'âœ… Build completed successfully!'",
    "_____________ TESTING _____________": "",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --no-file-parallelism",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:ci": "pnpm test:coverage && pnpm test:e2e",
    "_____________ CODE QUALITY _____________": "",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:ls": "ls-lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate": "concurrently \"pnpm type-check\" \"pnpm lint\" \"pnpm format:check\" \"pnpm lint:ls\"",
    "fix-all": "pnpm lint:fix && pnpm format",
    "_____________ DEPENDENCIES _____________": "",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update --interactive --latest",
    "deps:audit": "pnpm audit",
    "deps:audit:fix": "pnpm audit --fix",
    "_____________ SHADCN/UI _____________": "",
    "ui:add": "pnpm dlx shadcn@latest add",
    "ui:init": "pnpm dlx shadcn@latest init",
    "ui:update": "pnpm dlx shadcn@latest update",
    "ui:diff": "pnpm dlx shadcn@latest diff",
    "_____________ UTILITIES _____________": "",
    "clean": "pnpm dlx rimraf dist coverage playwright-report test-results .eslintcache",
    "clean:all": "pnpm clean && pnpm dlx rimraf node_modules pnpm-lock.yaml",
    "reset": "pnpm clean:all && pnpm install",
    "prepare": "husky",
    "postinstall": "pnpm exec playwright install chromium",
    "_____________ DEPLOYMENT _____________": "",
    "deploy:preview": "pnpm build && vercel --prebuilt",
    "deploy:production": "pnpm build && vercel --prebuilt --prod",
    "_____________ DEVELOPMENT TOOLS _____________": "",
    "generate:route": "pnpm dlx @tanstack/router-cli generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "pnpm dlx http-server storybook-static"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-query-devtools": "^5.67.1",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.2.7",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@ls-lint/ls-lint": "^2.3.0",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@tanstack/eslint-plugin-query": "^5.67.1",
    "@tanstack/router-devtools": "^1.121.8",
    "@tanstack/router-vite-plugin": "^1.121.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.14.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.4.3",
    "msw": "^2.8.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.18.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "web-vitals": "^5.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild", "sharp"],
    "overrides": {
      "@types/react": "^19.0.10",
      "@types/react-dom": "^19.0.4"
    },
    "peerDependencyRules": {
      "ignoreMissing": ["react", "react-dom"]
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{json,css,md}": ["prettier --write"]
  },
  "msw": {
    "workerDirectory": ["public"]
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}
