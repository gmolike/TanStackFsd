{
  "name": "ts-fsd-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm clean:dist && pnpm validate",
    "build": "tsc && vite build",
    "postbuild": "echo 'Build completed successfully!'",
    "preview": "vite preview",
    "_________________ Tests __________________": "",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --threads=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:8090",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:ci": "pnpm test:coverage && pnpm test:e2e",
    "_________________ OpenApi __________________": "",
    "openapi:generate": "openapi-generator-cli generate -i http://localhost:3000/api-docs/swagger.json -g typescript-axios -o src/shared/api/generated",
    "openapi:clean": "rimraf src/shared/api/generated",
    "openapi:regenerate": "npm run openapi:clean && npm run openapi:generate",
    "_________________ ShadCN __________________": "",
    "ui:add": "pnpm dlx shadcn@latest add",
    "ui:init": "pnpm dlx shadcn@latest init",
    "ui:update": "pnpm dlx shadcn@latest update",
    "ui:diff": "pnpm dlx shadcn@latest diff",
    "ui:list": "pnpm dlx shadcn@latest list",
    "_________________ Linter __________________": "",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix",
    "lint:ls": "ls-lint",
    "lint:report": "eslint --ext .ts,.tsx src/ ",
    "lint:staged": "lint-staged",
    "_________________ Format __________________": "",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json,md}\"",
    "format:json": "prettier --write \"**/*.json\" --loglevel warn",
    "lint:json": "eslint --ext .json --fix --ignore-pattern 'package*.json' --ignore-pattern 'node_modules/**' .",
    "_________________ TypeCheck  __________________": "",
    "type-check": "tsc -b",
    "type-check:watch": "tsc --watch",
    "_________________ Validate __________________": "",
    "validate": "pnpm exec concurrently \"pnpm type-check\" \"pnpm lint:report\" \"pnpm format:check\" \"pnpm lint:ls\"",
    "fix-all": "pnpm lint:fix && pnpm format",
    "_________________ Pre __________________": "",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "_________________ Setup __________________": "",
    "setup": "pnpm install && pnpm prepare",
    "reset": "pnpm clean && pnpm setup",
    "_________________ Clean __________________": "",
    "clean": "pnpm dlx rimraf dist node_modules",
    "clean:dist": "pnpm dlx rimraf dist",
    "_________________ CLI __________________": "",
    "start": "pnpm build && pnpm preview",
    "dev:full": "pnpm type-check && pnpm exec concurrently \"pnpm dev\" \"pnpm type-check:watch\"",
    "ci": "pnpm clean && pnpm validate && pnpm build",
    "pre-push": "pnpm validate"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@tanstack/react-router": "^1.111.11",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.6.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@faker-js/faker": "^9.8.0",
    "@ls-lint/ls-lint": "^2.2.3",
    "@playwright/test": "^1.48.0",
    "@tanstack/config": "^0.16.3",
    "@tanstack/eslint-config": "^0.1.0",
    "@tanstack/eslint-plugin-query": "^5.66.1",
    "@tanstack/publish-config": "^0.1.0",
    "@tanstack/router-devtools": "^1.111.11",
    "@tanstack/router-vite-plugin": "^1.111.12",
    "@tanstack/typedoc-config": "^0.1.0",
    "@tanstack/vite-config": "^0.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "shadcn": "latest",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint --fix", "prettier --write"]
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild", "sharp"],
    "overrides": {
      "@types/react": "^19.0.10",
      "@types/react-dom": "^19.0.4"
    }
  },
  "ignores": [
    "node_modules/**",
    "dist/**",
    "build/**",
    ".eslintrc.cjs",
    ".husky/**",
    "**/*.css",
    "**/*.scss"
  ]
}
